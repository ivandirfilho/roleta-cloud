{
  "provider": "evolution",
  "version": "1.0",
  "selectors": {
    "results": "[data-role='recent-number']",
    "betSpots": "[data-bet-spot-id]",
    "gameStatus": "[class*='trafficLightText']",
    "balance": "[data-role='balance-label-value']",
    "totalBet": "[data-role='total-bet-label-value']",
    "chips": "[data-role='chip']",
    "chipWrapper": "[data-role='chip-stack-wrapper']"
  },
  "data": {
    "monitoring": {
      "gameStatusSelectors": [
        "[class*='trafficLightText']",
        "[class*='statusMessage']",
        "[class*='betting-status']",
        "[data-role='game-message']"
      ],
      "balanceSelector": "[data-role='balance-label-value']",
      "betSelector": "[data-role='total-bet-label-value']",
      "chipSelectors": {
        "active": "[data-role='selected-chip'] [data-role='chip']",
        "wrapper": "[data-role='chip-stack-wrapper']"
      },
      "timerSelectors": [
        "[data-role='circle-timer']",
        "[class*='circle-timer']",
        "[class*='betting-timer']"
      ]
    }
  },
  "detection": {
    "urlPatterns": [
      "evo-games.com",
      "evolution"
    ],
    "gameStatusOpen": [
      "FAÇAM SUAS APOSTAS",
      "PLACE YOUR BETS"
    ],
    "gameStatusClosed": [
      "NÃO ACEITAMOS MAIS APOSTAS",
      "NO MORE BETS"
    ]
  },
